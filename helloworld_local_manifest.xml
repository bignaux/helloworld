<?xml version="1.0" encoding="UTF-8"?>
<!-- see https://gerrit.googlesource.com/git-repo/+/master/docs/manifest-format.md -->
<manifest>

  <remote name="ps2max"      fetch="https://gitlab.com/ps2max" />
  <remote name="ps2dev"      fetch="https://github.com/ps2dev" />
  <remote name="pspdev"      fetch="https://github.com/pspdev" />
  <remote name="ps2homebrew" fetch="https://github.com/ps2homebrew" />
  <remote name="AKuHAK"      fetch="https://github.com/AKuHAK" />
  <remote name="frno7"       fetch="https://github.com/frno7"  />
  <remote name="fjtrujy"     fetch="https://github.com/fjtrujy" />
  <remote name="bignaux"     fetch="https://github.com/bignaux" />
  <remote name="github"      fetch="https://github.com" />

  <!-- build Tools -->
  <project path="tools/clang-format-lint-action.git" name="DoozyX/clang-format-lint-action" revision="master" remote="github"/>

  <!-- Libraries (PS2/PSP) -->
  <project path="libs/lwnbd"       name="lwnbd.git"      revision="master"            remote="bignaux" />

  <!-- Applications -->
  <project path="apps/helloworld"       name="helloworld.git"      revision="master"            remote="bignaux" />
  <project path="apps/open-ps2-loader"   name="Open-PS2-Loader.git" revision="master"            remote="ps2homebrew" />

  <!-- Assets -->
  <project path="assets/opl-lng"   name="Open-PS2-Loader-lang.git"      revision="main"            remote="ps2homebrew" />

</manifest>
